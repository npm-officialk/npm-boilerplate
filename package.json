{
    "name": "@officialk/template-npm-package",
    "version": "0.0.0-development",
    "description": "A template repository to get quickly setup to create new NPM template-npm-packages.",
    "author": {
        "name": "officialk",
        "email": "karthik@officialk.dev"
    },
    "license": "ISC",
    "keywords": [],
    "homepage": "npm.officialk.dev/template-npm-package",
    "repository": {
        "type": "git",
        "url": "https://github.com/officialk/npm-template"
    },
    "bugs": {
        "url": "https://github.com/officialk/npm-template/issues/new"
    },
    "private": false,
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "tag": "latest"
    },
    "main": "./dist/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "require": {
                "types": "./dist/types/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist/**/*"
    ],
    "bin": "./dist/index.js",
    "scripts": {
        "//dev": "echo 'commands used in development'",
        "prestart": "yarn build:dist",
        "start": "node ./dist/index.js",
        "start:dev": "nodemon ./src/index.ts",
        "start:test": "concurrently --kill-others \"yarn check:coverage --watchAll\" \"yarn start:server -p 8765 ./coverage/lcov-report/\"",
        "poststart:test": "rimraf coverage",
        "prestart:docs": "yarn build:docs",
        "start:docs": "yarn start:server -p 9876 ./docs",
        "poststart:docs": "rimraf docs",
        "start:server": "http-server -d false -o /index.html --no-dotfiles -e html --cors -i false",
        "//fixes": "echo 'commands to fix the problems before checking for them'",
        "fix:format": "prettier --ignore-unknown --write ./",
        "fix:lint": "eslint --fix ./src/**/*",
        "fix:fortify": "snyk fix",
        "fix:audit": "yarn audit fix --groups 'dependencies' --force",
        "//builds": "echo 'update the codebase to either format,lint or build'",
        "build": "yarn build:dist && yarn build:docs",
        "postbuild": "yarn build:minify",
        "prebuild:dist": "rimraf dist",
        "build:dist": "tsc",
        "prebuild:docs": "rimraf docs",
        "build:docs": "jsdoc -c jsdoc.json",
        "build:minify": "minify-all-js ./dist -j -M -m",
        "//checks": "echo 'run checks to ensure QA and working'",
        "check:format": "prettier --check ./",
        "check:quality": "eslint --output-file QA/index.html --format html",
        "check:test": "jest --passWithNoTests",
        "check:coverage": "jest --passWithNoTests --coverage",
        "check:vunerability": "snyk code test --severity-level=low --all-projects ./src",
        "check:license": "snyk test",
        "check:audit": "yarn audit --groups 'dependencies'",
        "post:check:audit": "snyk monitor",
        "check:all": "yarn check:format && yarn check:quality && yarn check:test && yarn check:coverage && yarn check:vunerability && yarn check:license && yarn check:audit",
        "//misc": "echo 'Other commands used in the backend of development'",
        "commit": "git-cz",
        "prepare": "husky install",
        "postprepare": "git config --local core.hooksPath .husky",
        "lint-staged": "lint-staged -c .lintstagedrc",
        "release": "semantic-release --extends ./.releaserc.json",
        "prepack": "yarn build",
        "setup:labels": "github-label-sync -l ./github/labels.yml officialk/template-npm-package"
    },
    "dependencies": {},
    "devDependencies": {
        "jest": "^29.3.1",
        "husky": "^8.0.3",
        "jsdoc": "^4.0.0",
        "rimraf": "^3.0.2",
        "dotenv": "^16.0.3",
        "eslint": "^8.29.0",
        "ts-node": "^10.9.1",
        "ts-jest": "^29.0.5",
        "taffydb": "^2.7.3",
        "nodemon": "^2.0.20",
        "prettier": "2.8.1",
        "commitizen": "^4.2.6",
        "typescript": "^4.9.4",
        "better-docs": "^2.7.2",
        "@types/jest": "^29.2.5",
        "lint-staged": "^13.1.0",
        "http-server": "^14.1.1",
        "concurrently": "^7.6.0",
        "minify-all-js": "^0.1.9",
        "@commitlint/cli": "^17.4.2",
        "semantic-release": "^18.0.1",
        "github-label-sync": "^2.2.0",
        "eslint-config-node": "^4.1.0",
        "eslint-plugin-import": "^2.26.0",
        "semantic-release-cli": "^5.4.4",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/npm": "^8.0.3",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jsdoc-plugin-typescript": "^2.2.0",
        "@semantic-release/github": "^8.0.7",
        "cz-conventional-changelog": "^3.3.0",
        "@typescript-eslint/parser": "^5.48.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "@semantic-release/changelog": "^6.0.2",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "@commitlint/config-conventional": "^17.4.2",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@semantic-release/commit-analyzer": "^9.0.2",
        "@semantic-release/release-notes-generator": "^10.0.3"
    },
    "resolutions": {}
}
